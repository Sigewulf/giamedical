            <% if (sent === 'email') { %>
              <div class="col-lg-6 d-flex justify-content-center module">
                <div class="alert-success d-flex align-items-center justify-content-center p-5 w-100 text-center">
                  <span>You message was successfully sent. Please allow up to 72 hours for a replay. Thank you.</span>
                </div>
              </div>
            <% } else { %>
              <div class="col-lg-6 module">
                <form action="/contact" method="post">
                  <input type="hidden" name="_csrf" value="<%= csrfToken %>">

                  <div class="mb-3">
                    <label for="email" class="form-label" aria-label="Email Address"></label>
                    <input type="email" class="form-control" name="email" id="email" aria-describedby="emailHelp" placeholder="Email" required>
                  </div>

                  <div class="mb-3">
                    <label for="name" class="form-label" aria-label="Name"></label>
                    <input type="text" class="form-control" name="name" id="name" placeholder="Name" required>
                  </div>

                  <div class="mb-3">
                    <label for="subject" class="form-label" aria-label="Subject"></label>
                    <select class="form-select text-muted-black" style="height: 43px" name="subject" id="subject" aria-label="Select Subject" required>
                      <option value="" disabled selected hidden>Please Select</option>
                      <option value="general">General</option>
                      <option value="parts-ordering">Parts Ordering</option>
                      <option value="support">Tech Support</option>
                    </select>
                  </div>

                  <div class="mb-3">
                    <label for="message" class="form-label">Message</label>
                    <textarea type="text" class="form-control" name="message" id="message" rows="10"  placeholder="Message..." required></textarea>
                    <div class="form-text">We'll never share your information with anyone else.</div>
                  </div>

                  <div class="mb-3 form-check">
                    <input type="checkbox" class="form-check-input" id="exampleCheck1" required>
                    <label class="form-check-label" for="exampleCheck1">I agree with the <a class="text-black" href="">privacy policy</a> and <a class="text-black" href="">terms of service.</a></label>
                  </div>

                  <div class="g-recaptcha mb-3" data-sitekey="6LfytJIaAAAAAKhyWI4saFSea0Ey4WgxDs3KJcYB"></div>

                  <button id="form-btn" type="submit" class="btn d-block w-100 shadow-sm">Submit</button>
                  <span class="form-text d-block text-center">Please allow up to 24 hours for reply.</span>

                </form>
              </div>
            <% } %>
